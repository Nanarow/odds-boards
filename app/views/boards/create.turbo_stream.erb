<%= turbo_stream.prepend 'boards', @board %>
<%= turbo_stream.update 'board-form' %>
<%= turbo_stream.update "categories" do %>
  <%= render @categories %>
<% end %>
<%= turbo_stream.update "tags" do %>
  <%= render 'tags/list', tags: @tags %>
<% end %>
<%= turbo_stream.update "notification" do %>
  <%= render 'shared/notification' %>
<% end %>